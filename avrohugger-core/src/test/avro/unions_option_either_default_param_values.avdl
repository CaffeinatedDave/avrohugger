  record CopY {
    union { Event4, Event5, Event6 } value = { "index" : 8 };
  }

  record CopZ {
    union { Event7, Event8, Event9 } value = { "index" : 9 };
  }

  record ShouldRenderAsCoproductOfCoproduct {
    union { CopX, CopY, CopZ } value = { "value" : { "index" : 10 }};
